        <name>RFIHandler</name>
        <script><![CDATA[var RFIHandler = Class.create();
RFIHandler.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

    type: 'RFIHandler',

	getPreviousApprovers : function(){
		var approver = []
		var parentId = this.getParameter('sysparm_parentid')
		
		var gr = new GlideRecord('sysapproval_approver')
		gr.addQuery('sysapproval',parentId)
		gr.addQuery('state','approved')
		gr.query()

		while(gr.next()){
			approver.push({
				displayValue : gr.approver.name.getDisplayValue(),
				value : gr.approver.sys_id.getDisplayValue()
			})
		}
		return JSON.stringify(approver)
	},

	handleRFI : function(){
		gs.info('handle function called')
		return
	}
});]]></script>
